{
  "name": "yoummday/refactoring",
  "description": "Coding interview refactoring task",
  "type": "project",
  "require": {
    "progphil1337/simple-react-app": "dev-main",
    "webmozart/assert": "^1.11",
    "guzzlehttp/guzzle": "8.0.x-dev"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "Test\\": "Test/"
    },
    "classmap": [
      "src/"
    ]
  },
  "authors": [
    {
      "name": "Philipp Lohmann",
      "email": "philipp.lohmann@yoummday.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "10.5.x-dev",
    "friendsofphp/php-cs-fixer": "dev-master",
    "phpstan/phpstan": "2.0.x-dev"
  },
  "scripts": {
    "cs": "php-cs-fixer fix",
    "phpstan": "phpstan analyse --configuration=phpstan.neon",
    "tests": "phpunit Test"
  }
}
